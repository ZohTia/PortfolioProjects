{
  "datasets": [
    {
      "name": "b6890bfd",
      "displayName": "Location Data",
      "queryLines": [
        "SELECT longitude, latitude, 'GHG emissions mtons CO2e' as Emissions\n",
        "FROM emissions.default.emissions_data"
      ]
    },
    {
      "name": "f0802801",
      "displayName": "Emissions per person",
      "queryLines": [
        "SELECT county_state_name, \n",
        "population, \n",
        "CAST(\n",
        "  REPLACE (`GHG emissions mtons CO2e`, ',', '') \n",
        "  AS double\n",
        "  ) / NULLIF(\n",
        "    CAST(\n",
        "      REPLACE(population, ',', '') AS double\n",
        "    ), 0\n",
        "  )AS emissions_per_person \n",
        "  FROM emissions.default.emissions_data\n",
        "  ORDER BY emissions_per_person DESC\n"
      ]
    },
    {
      "name": "9e2225bf",
      "displayName": "Emissions per state",
      "queryLines": [
        "SELECT state_abbr,\n",
        "        SUM(CAST(\n",
        "          REPLACE(`GHG emissions mtons CO2e`, ',', '') \n",
        "          AS DOUBLE\n",
        "        )) AS Total_Emissions\n",
        "FROM emissions_data\n",
        "GROUP BY state_abbr\n",
        "ORDER BY Total_Emissions DESC\n",
        "LIMIT 10"
      ],
      "catalog": "emissions",
      "schema": "default"
    },
    {
      "name": "b1fed1ad",
      "displayName": "County Shaming",
      "queryLines": [
        "SELECT county_state_name,\n",
        "       population,\n",
        "       CAST(\n",
        "        REPLACE (`GHG emissions mtons CO2e`, ',', '')\n",
        "        AS DOUBLE) AS Total_Emissions\n",
        "FROM emissions_data\n",
        "ORDER BY Total_Emissions DESC\n",
        "LIMIT 10"
      ],
      "catalog": "emissions",
      "schema": "default"
    }
  ],
